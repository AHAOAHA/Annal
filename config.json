{
    "plugins": [
        {
            "name": "zcomet",
            "repo": "agkozak/zcomet",
            "links": [
                {
                    "dst": "plugins/zcomet",
                    "link": "~/.zcomet/bin"
                }
            ],
            "actions": [
                "clone",
                "link"
            ]
        },
        {
            "name": "JumpTo",
            "repo": "ahaooahaz/JumpTo",
            "links": [
                {
                    "dst": "plugins/JumpTo/jt.sh",
                    "link": "~/.local/bin/jt"
                }
            ],
            "actions": [
                "clone",
                "link"
            ]
        },
        {
            "name": "tmux",
            "repo": "gpakosz/.tmux",
            "links": [
                {
                    "dst": "plugins/tmux",
                    "link": "~/.tmux"
                },
                {
                    "dst": "plugins/tmux/.tmux.conf",
                    "link": "~/.tmux.conf"
                },
                {
                    "dst": "configs/.tmux.conf.local",
                    "link": "~/.tmux.conf.local"
                }
            ],
            "actions": [
                "clone",
                "link"
            ]
        },
        {
            "name": "rime",
            "links": [
                {
                    "dst": "configs/rime/ahaooahaz_pinyin.dict.yaml",
                    "link": "~/.config/ibus/rime/ahaooahaz_pinyin.dict.yaml"
                },
                {
                    "dst": "configs/rime/ahaooahaz.extended.dict.yaml",
                    "link": "~/.config/ibus/rime/ahaooahaz.extended.dict.yaml"
                },
                {
                    "dst": "configs/rime/default.custom.yaml",
                    "link": "~/.config/ibus/rime/default.custom.yaml"
                },
                {
                    "dst": "configs/rime/luna_pinyin_simp.custom.yaml",
                    "link": "~/.config/ibus/rime/luna_pinyin_simp.custom.yaml"
                }
            ],
            "actions": [
                "link"
            ],
            "childrens": [
                {
                    "name": "rime-dict",
                    "repo": "ssnhd/rime",
                    "links": [
                        {
                            "dst": "plugins/rime-dict/配置文件/easy_en.dict.yaml",
                            "link": "~/.config/ibus/rime/easy_en.dict.yaml"
                        },
                        {
                            "dst": "plugins/rime-dict/配置文件/luna_pinyin.dict.yaml",
                            "link": "~/.config/ibus/rime/luna_pinyin.dict.yaml"
                        },
                        {
                            "dst": "plugins/rime-dict/配置文件/luna_pinyin.extended.dict.yaml",
                            "link": "~/.config/ibus/rime/luna_pinyin.extended.dict.yaml"
                        },
                        {
                            "dst": "plugins/rime-dict/配置文件/luna_pinyin.sogou.dict.yaml",
                            "link": "~/.config/ibus/rime/luna_pinyin.sogou.dict.yaml"
                        }
                    ],
                    "actions": [
                        "clone",
                        "link"
                    ]
                },
                {
                    "name": "rime-emoji",
                    "repo": "rime/rime-emoji",
                    "links": [
                        {
                            "dst": "plugins/rime-emoji/opencc",
                            "link": "~/.config/ibus/rime/opencc"
                        }
                    ],
                    "actions": [
                        "clone",
                        "link"
                    ]
                }
            ]
        },
        {
            "name": "kitty",
            "links": [
                {
                    "dst": "configs/kitty",
                    "link": "~/.config/kitty"
                }
            ],
            "actions": [
                "link"
            ]
        },
        {
            "name": "vim",
            "links": [
                {
                    "dst": "configs/rcs/.vimrc",
                    "link": "~/.vimrc"
                },
                {
                    "dst": "configs/nvim",
                    "link": "~/.config/nvim"
                }
            ],
            "actions": [
                "link"
            ]
        },
        {
            "name": "env",
            "links": [
                {
                    "dst": "configs/rcs/.bash_profile",
                    "link": "~/.bash_profile"
                },
                {
                    "dst": "configs/env.rc",
                    "link": "~/.envrc"
                },
                {
                    "dst": "configs/rcs/.profile",
                    "link": "~/.profile"
                }
            ],
            "actions": [
                "link"
            ]
        },
        {
            "name": "zsh",
            "links": [
                {
                    "dst": "configs/zsh",
                    "link": "~/.config/zsh"
                },
                {
                    "dst": "configs/zsh/.zshenv",
                    "link": "~/.zshenv"
                }
            ],
            "actions": [
                "link"
            ]
        },
        {
            "name": "annal",
            "links": [
                {
                    "dst": "binaries/annal",
                    "link": "~/.local/bin/annal"
                }
            ],
            "actions": [
                "link"
            ]
        },
        {
            "name": "scripts",
            "links": [
                {
                    "dst": "scripts/docker-wechat.sh",
                    "link": "~/.local/bin/docker-wechat.sh"
                },
                {
                    "dst": "scripts/docker-wechat.work.sh",
                    "link": "~/.local/bin/docker-wechat.work.sh"
                }
            ],
            "actions": [
                "link"
            ]
        }
    ]
}
